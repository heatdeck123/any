    })
    setclipboard(key)
end)

loginButton.MouseButton1Click:Connect(function()
    local enteredKey = keyTextBox.Text

    if enteredKey == uniqueKey then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Mbm Hub",
            Text = "wait script...",
            Duration = 4
        })
        loadstring(game:HttpGet("https://raw.githubusercontent.com/m1M-Plqer819/any/main/mbmxhub/n3wu1"))()
        gui:Destroy()
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Mbm Hub",
            Text = "have fun",
            Duration = 4
        })
    end
end)

